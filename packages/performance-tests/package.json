{
  "name": "transformer-performance-tests",
  "private": true,
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc 1>&2",
    "build:ci": "npm run -s build",
    "clean": "rimraf lib",
    "lint": "eslint \"./test/**/*.ts\" 1>&2",
    "test": "mocha --delay --timeout 300000 --require ts-node/register test/**/*.test.ts",
    "prettier:format": "prettier \"./test/**/*.ts\" --write",
